<tr>
 <th><%= form.label :title, "タイトル" %></th><br>
 <td><%= form.text_field :title %></td>
</tr>

<tr>
 <th><%= form.label :body, "詳細" %></th><br>
 <td><%= form.text_area :body, row: 10, cols: 30 %></td>
</tr>
<tr>
 <th><%= form.label :released_at, "登録日" %></th>
 <td><%= form.datetime_select :released_at,
 start_year: 2000, end_year: Time.now.year + 1,
 use_month_number: true %></td>
</tr>


